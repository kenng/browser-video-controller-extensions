{
    "name": "my-video-controller",
    "version": "1.0.0",
    "description": "Browser extensions for video controller",
    "main": "index.js",
    "scripts": {
        "dev": "webpack serve --config webpack.dev.js",
        "build": "npm run clean && webpack --config webpack.prod.js",
        "clean": "rm -rf ./build/*.js ./build/*.html",
        "httpd-on-prod": "http-server -p 8080 ./build"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/kenng/browser-video-controller-extensions.git"
    },
    "keywords": [
        "video",
        "speed",
        "controller",
        "browser",
        "chrome",
        "extensions"
    ],
    "author": "Ken Ng",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kenng/browser-video-controller-extensions/issues"
    },
    "homepage": "https://github.com/kenng/browser-video-controller-extensions#readme",
    "devDependencies": {
        "@swc/core": "^1.2.62",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "typescript": "^4.3.4",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "cypress": "^7.6.0",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-loader": "^4.0.2",
        "html-webpack-plugin": "^5.3.2",
        "swc-loader": "^0.1.14",
        "webpack": "^5.40.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2",
        "http-server": "^0.12.3"
    },
    "dependencies": {}
}
